{
	"name": "@ctx-core/esm",
	"version": "4.3.2",
	"description": "ctx-core esm",
	"keywords": [
		"ctx-core",
		"esm"
	],
	"homepage": "https://github.com/ctx-core/esm#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/esm/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/esm/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@"
	},
	"dependencies": {
		"esm": "^3.2.25"
	},
	"devDependencies": {
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
